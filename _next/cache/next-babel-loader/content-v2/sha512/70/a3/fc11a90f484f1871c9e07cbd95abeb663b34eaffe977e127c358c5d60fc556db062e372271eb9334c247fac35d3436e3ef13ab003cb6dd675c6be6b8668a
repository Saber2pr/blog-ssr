{"ast":null,"code":"import moment from 'moment';\nimport { isNullOrUndefined } from './is';\nexport var getNowMon = function getNowMon() {\n  return moment();\n};\nexport var getNow = function getNow() {\n  var format = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 'YYYY-MM-DD HH:mm:ss';\n  return getNowMon().format(format);\n};\nexport var formatTimestamp = function formatTimestamp(timestamp) {\n  var format = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 'MM-DD HH:mm';\n\n  if (isNullOrUndefined(timestamp)) {\n    return '-';\n  }\n\n  if (String(timestamp).length === 10) {\n    timestamp = timestamp * 1000;\n  }\n\n  return moment(timestamp).format(format);\n};\nexport var formatSeconds = function formatSeconds(seconds) {\n  var split = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : ':';\n\n  if (isNullOrUndefined(seconds) || Number.isNaN(seconds)) {\n    return '-';\n  }\n\n  var h = Math.floor(seconds / 3600);\n  var m = Math.floor(seconds % 3600 / 60);\n  var s = Math.floor(seconds % 60);\n  return [h, m, s].map(function (v) {\n    return String(v).padStart(2, '0');\n  }).join(split);\n};\nexport var formatTime = function formatTime(time) {\n  return \"\".concat(time.slice(0, 4), \"/\").concat(time.slice(4));\n};","map":null,"metadata":{},"sourceType":"module"}