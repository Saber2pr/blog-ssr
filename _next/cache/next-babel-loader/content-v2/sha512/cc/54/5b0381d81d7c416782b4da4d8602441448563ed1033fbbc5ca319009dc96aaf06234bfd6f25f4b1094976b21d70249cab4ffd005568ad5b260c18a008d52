{"ast":null,"code":"var __jsx = React.createElement;\nimport './style.less';\nimport { useRouter } from 'next/router';\n/*\n * @Author: saber2pr\n * @Date: 2019-07-21 18:41:06\n * @Last Modified by: saber2pr\n * @Last Modified time: 2019-11-18 13:32:11\n */\n\nimport React from 'react';\nimport { Ul } from '../ul';\nexport function Tree({\n  from: root,\n  map: render,\n  depth = 0,\n  selectBtn\n}) {\n  const children = root.children;\n  const router = useRouter();\n  if (root) if (children) {\n    const childNodes = children.map((node, key) => __jsx(\"li\", {\n      className: \"Tree-Li\",\n      key: key\n    }, __jsx(Tree, {\n      from: node,\n      map: render,\n      depth: depth + 1,\n      selectBtn: selectBtn\n    })));\n    if (depth === 0) return __jsx(React.Fragment, null, childNodes);\n\n    if (root.path) {\n      root.expand = decodeURI(router.asPath).startsWith(root.path);\n    }\n\n    return __jsx(Ul, {\n      node: root,\n      render: render,\n      index: depth,\n      selectBtn: selectBtn\n    }, childNodes);\n  } else {\n    return render(root, depth);\n  }\n}","map":null,"metadata":{},"sourceType":"module"}